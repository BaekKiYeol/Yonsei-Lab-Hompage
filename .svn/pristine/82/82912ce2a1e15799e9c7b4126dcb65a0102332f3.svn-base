  .cont2-vote__wrap .vote.swiper-slide { flex-shrink: 0; width: 303px;}
  .cont2-vote__wrap.swiper-wrapper {margin-top: 30px;}
  .cont2-service {margin-top: 100px;}
  .cont2-service > h3 {font-size: 24px;}

  /* con_3 */
  .cont3-list {flex-direction: column;margin-top: 70px;}
  .cont3-list > li:not(:last-child) {margin-right: 0; margin-bottom: 30px;}
  .cont3-list > li h4 {margin-bottom: 20px;}
  .cont3-list2 { flex-direction: column;}
  .cont3-list2 li:not(:last-child) {margin-bottom: 60px;}

  /* con_4 */
  .cont4-slide {margin: 0 -15px;margin-top: 100px; padding: 40px 0; padding-bottom: 80px;}
  .cont-area.con4 .cont-area__title span {display: none;}
  .cont-area__txt > a {font-size: 18px;margin: 0 20px;margin-top: 15px;}
  .cont4-slide_wrap .swiper-slide img {width: 100%;}
  .cont-area__img img.pc {display: none;}
  .cont-area__img img.mobile {display: block;}
  .cont-area__img img.marbot {margin-bottom: 25px;}
  .cont4-slide_title {flex-direction: column; align-items: center;justify-content: center;}
  .slide_title-left {text-align: center;}
  .slide_title-left > h4 { padding-left: 0;font-size: 20px;}
  .slide_title-left > p {font-size: 10px;padding-left: 0;}
  .slide_title-left > h4::before {background-size: cover;background-position: center;width: 23px;height: 27px;left: -25px;}
  .slide_title-right {margin-top: 30px;}
  .cont4-slide_wrap .swiper-button-prev {width: 15px;height: 15px; top: auto; bottom: 3px; margin-top: 0; left: 20px; z-index: 99;}
  .cont4-slide_wrap .swiper-button-next { width: 15px; height: 15px; top: auto; bottom: 3px; margin-top: 0; right: 20px; z-index: 99;}
  .cont4-slide_wrap {padding: 0 15px; padding-bottom: 30px;}
  .cont4-slide_wrap:last-child {margin-top: 90px;}
  .cont4-map > h3 {font-size: 22px;}
  .cont4-process > h3 {font-size: 22px; margin-bottom: 50px;}
  .process_list_ {font-size: 14px;}
  .process_list { width: 151px; height: 151px;}
  .cont4-map img.pc {display: none;}
  .cont4-map img.mobile {display: block;}
  .cont4-process {margin-top: 80px;}

}