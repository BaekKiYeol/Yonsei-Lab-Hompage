/*sftext{*/
.sftext {
    display: inline-block;
    vertical-align: top;
    position: relative;
}


.sftext>input {
    height: 100%;
    width: 100%;
    font-size: 20px;
    font-weight: 500;
    color: #2E2E2E;
    background-color:#ffffff;
    border-color: #2E2E2E;
    border-width: 1px;
    border-style: solid;
    border-radius: inherit;
    padding: 0 20px;
}

.sftext>textarea {
    height: 100%;
    width: 100%;
    font-size: 20px;
    font-weight: 500;
    color: #2E2E2E;
    background-color:#ffffff;
    border-color: #2E2E2E;
    border-width: 1px;
    border-style: solid;
    border-radius: inherit;
    padding: 20px;
    resize: none;
}

/* IE9 */
.placeholder {
    color: #CECECE;
}

/* IE10*/
input::placeholder,
.sftel>input::placeholder,
textarea::placeholder{
    color: #CECECE;
    opacity: 1;
}
input::-webkit-input-placeholder,
.sftel>input::-webkit-placeholder,
textarea::-webkit-input-placeholder {
    color: #CECECE;
}
/* IE */
input:-ms-input-placeholder,
.sftel>input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #CECECE;
}

/* Firefox */
input:-mos-input-placeholder,
.sftel>input:-mos-input-placeholder,
textarea:-mos-input-placeholder {
    color: #CECECE;
}
input~label.likePlaceholder {
    font-size: 12px;
    font-weight: 300;
    color:#CECECE;
    position: absolute;
    left: 11px;
    top: 50%;
    transform: translateY(-50%);
}

input:placeholder-shown,
.sftel>input:placeholder-shown,
textarea:placeholder-shown{
    border-color: #CECECE;
}

/*}sftext*/



