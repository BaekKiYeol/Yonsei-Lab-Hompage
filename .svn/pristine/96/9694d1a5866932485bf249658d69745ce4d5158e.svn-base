.sect_searchList * {font-family: 'SUIT', sans-serif;}
.search-form .wrapper {
    max-width: 1240px;
    margin: 0 auto;
    margin-top: 80px;
    padding: 15px;
}
.total_sch_head_wrap .bottom {
    background: #F7F7F7;
    border-bottom: 1px solid #E4E4E4;
    min-height: 100px;
    justify-content: center;
    display: flex;
    align-items: center;
    margin-bottom: 40px;
}
.total_sch_head_wrap .bottom .bottom_txt {
    font-size: 18px;
    text-align: center;
    line-height: 1.5;
}
.total_sch_head_wrap .bottom span {
    font-weight: 600;
}
.total_sch_head_wrap .bottom .bottom_txt br {
    display: none;
}
.sect_searchList .total_sch_wrap .total_sch_middle_wrap {
    width: 100%;
}
.total_sch_middle_wrap .sch_result {
    width: 100%;
    display: flex;
    margin-bottom: 20px;
    align-items: center;
    position: relative;
}
.sch_result .sch_result_txt {
    font-size: 1.125rem;
    display: flex;
}
.sch_result .sch_result_txt p {
    margin-left: 3px;
    font-size: 1.125rem;
}
.sch_result span {
    font-weight: 600;
}
.sch_result .more {
    display: flex;
    position: absolute;
    top: 0;
    right: 0;
    font-size: 16px;
}
.list_category {
    background: #F7F7F7;
    border: 1px solid #E4E4E4;
    padding: 5px 15px;
    font-size: 0.875rem;
}
.sect_searchList .total_sch_wrap .total_sch_middle_wrap ul + ul {
    margin-top: 50px;
}
.sect_searchList .total_sch_wrap .total_sch_middle_wrap ul li + li {
    margin-top: 30px;
}
.sect_searchList .total_sch_wrap .total_sch_middle_wrap .list_tit, .sect_searchList .total_sch_wrap .total_sch_middle_wrap .list_cont {
    margin: 0 20px;
}
.sect_searchList .total_sch_wrap .total_sch_middle_wrap .list_tit {
    font-size: 1rem;
    font-weight: 600;
    padding: 15px 0 5px 0;
}
.sect_searchList .total_sch_wrap .total_sch_middle_wrap .list_cont {
    line-height: 1.5;
    font-size: 0.875rem;
}



@media screen and (max-width: 1250px){
    #sch_result {
        padding: 0 40px;
    }
    .search-form .wrapper {
        margin-top: 40px;
    }
}




@media screen and (max-width: 1180px){
    .search-form--tit {
        margin-bottom: 30px;
    }
    
    #sch_res_detail {
        margin-bottom: 40px;
    }
    .sch_res_list p {
    word-wrap: break-word;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    /* white-space: pre-wrap; */
    }
    /* search.php
    .search-form .wrapper {
        padding: 0 40px;
    } */
}


@media screen and (max-width:768px){
    #sch_result {
        padding: 0 16px;
    }
    .search-form .wrapper{}
    #sch_res_detail .sch_wr{width: 100%;}
    .search-form{padding-top: 50px;}
    .search-form--tit{font-size: 22px;}
    #sch_res_detail .frm_input{height: 50px;font-size: 14px;padding: 0 25px;}
    #sch_res_detail .btn_submit{width: 50px;height: 50px;}
    #sch_res_detail{margin-bottom: 25px;}
    #sch_res_ov br.mo{display: block;}
    #sch_res_ov{height: auto;padding: 15px 0;text-align: center;margin-bottom: 20px;}
    #sch_res_ov h2{line-height: 26px;font-size: 16px;}
    .sch_res_list h2{}
    .sch_res_list h2:before {
        width: 3px;
        height: 3px;
        margin-right: 5px;
    }
    .sch_more{font-size: 14px;top: 2px;}
    .sch_res_list p.search-lnb {
        height: 30px;
        padding: 0 9px;
        font-size: 12px;
    }
    
    .sch_res_list .sch_tit {font-size: 14px;}
    
    .search-result--cont {
    }
    
    .sch_res_list p {
        font-size: 14px;
        -webkit-line-clamp: 2;
    }
    .empty_list p {
        font-size: 16px;
    }
    #sch_res_board {display: flex;align-items: center;white-space: nowrap;overflow-x: auto;}

    #sch_res_board li {
        flex: 0 0 auto;
        width: 110px;
    }

    ul#sch_res_board::-webkit-scrollbar-thumb {
        background: #e4e4e4;
        width: auto;
    }

    ul#sch_res_board::-webkit-scrollbar {
        width: 3px;
        height: 3px;
    }

    #sch_res_board a {
        font-size: 14px;
    }

    /*search.php*/
    .search-form .wrapper {
        padding: 0 16px;
    }
    .total_sch_head_wrap .bottom .bottom_txt {
        font-size: 14px;
    }
    .total_sch_head_wrap .bottom .bottom_txt br {
        display: block;
    }
    .sch_result .sch_result_txt {
        font-size: 14px;
        display: unset;
        line-height: 1.7;
    }
    .sch_result .sch_result_txt p {
        font-size :14px;
        margin-left: 10px;
    }
    .sect_searchList .total_sch_wrap .total_sch_middle_wrap .list_tit, .sect_searchList .total_sch_wrap .total_sch_middle_wrap .list_cont {
        margin: 0 10px;
    }
}

@media screen and (max-width:500px){
    #sch_result {
        padding: 0;
    }
}